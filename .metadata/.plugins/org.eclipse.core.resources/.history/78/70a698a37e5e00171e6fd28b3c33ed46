package com;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

class Pair {
	int p1, p2;

	public Pair(int p1, int p2) {
		super();
		this.p1 = p1;
		this.p2 = p2;
	}

}

public class Solution {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int[] a = new int[n];
		for (int i = 0; i < n; i++) {
			a[i] = in.nextInt();
		}
		Arrays.sort(a);
		// System.out.println(ar[n/3]);
		int ldif = Integer.MAX_VALUE;
		ArrayList<Pair> list = null;
		for (int i = 0; i < n - 1; i++) {
			if (Math.abs(a[i + 1] - a[i]) < ldif) {
				list=new ArrayList<>();
				list.add(new Pair(a[i],a[i+1]));
				ldif=Math.abs(a[i + 1] - a[i]);
			} else if(Math.abs(a[i + 1] - a[i]) == ldif){
				list.add(new Pair(a[i],a[i+1]));
			}
		}
		for (int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i).p1+" "+list.get(i).p2);
		}
	}
}