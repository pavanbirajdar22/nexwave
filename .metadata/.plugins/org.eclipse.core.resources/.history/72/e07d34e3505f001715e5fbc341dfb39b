package com;

import java.util.Scanner;

class Pair {
	int p1, p2;

	public Pair(int p1, int p2) {
		super();
		this.p1 = p1;
		this.p2 = p2;
	}

}

public class Solution {
	static String super_digit(String s) {
		if (s.length() == 1) {
			return s;
		}
		int new_dig = 0;
		for (int i = 0; i < s.length(); i++) {
			int dig = ((int) s.charAt(i) - 48);
			new_dig += dig;
		}
		return super_digit(Long.toString(new_dig));
	}

	static String helper(String s,long k){
    	long new_dig = 0;
        if(s.length()==1){
            new_dig = Long.parseLong(s);
            new_dig *= k ;
            return super_digit(Long.toString(new_dig));
        }
        
        for(long i=0;i<s.length();i++){
            int dig = ((int)s.charAt(i)-48);
            new_dig+=dig;
        }
        return super_digit(Long.toString(new_dig*k));
    }

	public static void main(String[] args) {
		String s;
		Scanner scanner = new Scanner(System.in);
		s = scanner.nextLine();
		String[] split = s.split(" ");
		System.out.println(helper(split[0], Long.parseLong(split[1])));
		scanner.close();
	}
}