package com;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Scanner;

class Pair {
	long p1, p2;

	public Pair(long p1, long p2) {
		super();
		this.p1 = p1;
		this.p2 = p2;
	}

}

public class Solution {
	String super_digit(String s) {
		if (s.length() == 1) {
			return s;
		}
		long new_dig = 0;
		for (long i = 0; i < s.length(); i++) {
			long dig = ((long) s.charAt(0) - 48);
			new_dig += dig;
		}
		return super_digit(Integer.toString(new_dig));
	}

	String helper(String s,long k){
    	long new_dig = 0;
        if(s.length()==1){
            new_dig = Integer.parseInt(s);
            new_dig *= k ;
            return super_digit(Integer.toString(new_dig));
        }
        
        for(long i=0;i<s.length();i++){
            long dig = ((long)s.charAt(0)-48);
            new_dig+=dig;
        }
        return super_digit(Integer.toString(new_dig*k));
    }

	public static void main(String[] args) {

	}
}